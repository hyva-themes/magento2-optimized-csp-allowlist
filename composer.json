{
  "name": "hyva-themes/magento2-optimized-csp-allowlist",
  "description": "Be more precise on which CSP domains are allowed to be loaded",
  "type": "magento2-module",
  "require": {
    "magento/framework": "*",
    "magento/module-csp": "100.4.*"
  },
  "license": [
    "Open Software License (OSL)"
  ],
  "autoload": {
    "files": [
      "src/registration.php"
    ],
    "psr-4": {
      "Hyva\\OptimizedCspAllowlist\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Hyva\\OptimizedCspAllowlist\\": "tests/"
    }
  },
  "repositories": {
    "mageos": {
      "type": "composer",
      "url": "https://mirror.mage-os.org/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^12.2"
  },
  "config": {
    "allow-plugins": {
      "magento/composer-dependency-version-audit-plugin": false
    }
  }
}
